 				:array:<:ctr2:> <- :array:<:tempVar:>
 				:array:<:tempVar:> <- :temp: 
 			end check
 		end loop
 	end loop

 	start loop (since :ctr1: <- 0 until :ctr1: ?< :max: do tick(:ctr1:))
 		display :array:<:ctr1:>
 	end loop
